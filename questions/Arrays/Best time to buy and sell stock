class Solution {
    public int maxProfit(int[] arr) {
        int profit=0;
        int mini=arr[0];
        for(int i=0;i<arr.length;i++){
            int diff=arr[i]-mini;
            profit=Math.max(profit,diff);
            mini=Math.min(arr[i],mini);
        }
        return profit;
    }
}

//time complexity-O(n)
//space complexity-O(1)
